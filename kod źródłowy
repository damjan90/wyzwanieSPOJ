#include <iostream>

using namespace std;

int main()
{
    int tab[100];

    for(int i = 0; i < 100; i++)
    {
        cin>>tab[i];
    }
    cout<<endl;
    for(int i = 0; i < 100; i++)
    {
        if((tab[i]% 5 == 0) && (tab[i] % 3 == 0))
        {
            cout<<"SPOKOKOKO"<<endl;
        }

        else if (tab[i] % 5 == 0)
        {
            cout<<"SPOKO"<<endl;
        }

        else if (tab[i] % 3 == 0)
        {
            cout<<"KOKO"<<endl;
        }

        else
        {
            cout<<tab[i]<<endl;
        }

    }

}
